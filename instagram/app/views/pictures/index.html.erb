
<h1>Pictures#index</h1>
<p>Find me in app/views/pictures/index.html.erb</p>

<% if !@pictures.empty? %>
<% @pictures.reverse.each do |picture| %>
  <%= picture.caption %>
  <%= picture.created_at %>
  <%= link_to "View Picture #{picture.id}", picture_url(picture) %>
<% end %>
<% end %>

<%= link_to "Post Picture", new_picture_url %>
